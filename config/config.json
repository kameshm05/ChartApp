{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "chartapp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/chartapp/ChartappWebPart.js",
          "manifest": "./src/webparts/chartapp/ChartappWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js",
      "globalName": "jquery"
    },
    "canvas": {
      "path": "https://canvasjs.com/assets/script/canvasjs.min.js",
      "globalName": "canvas"
    },
    "d3layout": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/d3-cloud/1.2.5/d3.layout.cloud.min.js",
      "globalName": "d3layout"
    },
    "d3": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.min.js",
      "globalName": "d3"
    },
    "d3old": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/d3/2.5.1/d3.min.js",
      "globalName": "d3old"
    },
    "amchartsjs": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/amcharts/3.21.15/amcharts.js",
      "globalName": "amchartsjs"
    },
    "ampiechart": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/amcharts/3.21.15/pie.js",
      "globalName": "ampiechart"
    }
  },
  "localizedResources": {
    "ChartappWebPartStrings": "lib/webparts/chartapp/loc/{locale}.js"
  }
}